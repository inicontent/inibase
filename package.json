{
	"name": "inibase",
	"version": "1.0.0-rc.61",
	"author": {
		"name": "Karim Amahtil",
		"email": "karim.amahtil@gmail.com"
	},
	"repository": "inicontent/inibase",
	"main": "./dist/index.js",
	"exports": {
		".": "./dist/index.js",
		"./file": "./dist/file.js",
		"./config": "./dist/config.js",
		"./utils": "./dist/utils.js",
		"./utils.server": "./dist/utils.server.js"
	},
	"bugs": {
		"url": "https://github.com/inicontent/inibase/issues"
	},
	"description": "A file-based & memory-efficient, serverless, ACID compliant, relational database management system",
	"engines": {
		"node": ">=16"
	},
	"files": [
		"/dist"
	],
	"funding": "https://github.com/sponsors/inicontent",
	"homepage": "https://github.com/inicontent/inibase#readme",
	"keywords": [
		"nosql",
		"rdms",
		"database",
		"db",
		"mongoose",
		"relational",
		"local",
		"file",
		"storage",
		"json",
		"sqlite",
		"sql",
		"supabase",
		"better-sqlite",
		"mongodb",
		"firebase",
		"postgresql",
		"pocketbase"
	],
	"license": "MIT",
	"scripts": {
		"prepare": "tsc",
		"build": "tsc",
		"benchmark": "tsx ./benchmark/index",
		"benchmark:single": "tsx --expose-gc ./benchmark/single",
		"benchmark:bulk": "tsx --expose-gc ./benchmark/bulk"
	},
	"bin": {
		"inibase": "./dist/cli.js"
	},
	"type": "module",
	"types": "./dist",
	"typesVersions": {
		"*": {
			".": [
				"./dist/index.d.ts"
			],
			"file": [
				"./dist/file.d.ts"
			],
			"utils": [
				"./dist/utils.d.ts"
			],
			"config": [
				"./dist/config.d.ts"
			],
			"utils.server": [
				"./dist/utils.server.d.ts"
			]
		}
	},
	"devDependencies": {
		"@types/node": "^20.12.11",
		"tinybench": "^2.6.0"
	},
	"dependencies": {
		"dotenv": "^16.4.5",
		"inison": "^1.0.0-rc.2"
	}
}
